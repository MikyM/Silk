@namespace Silk.Dashboard.Components

@using Silk.Dashboard.Models

<MudGrid>
    <MudItem xs="12">
        <MudGrid Spacing="4" Justify="Justify.Center">
            @foreach (var owner in Owners)
            {
                <MudItem @key="@owner">
                    <OwnerCardView Owner="@owner" />
                </MudItem>
            }
        </MudGrid>
    </MudItem>
</MudGrid>

@code {

    [Parameter]
    public List<SilkOwner> Owners { get; set; }

}